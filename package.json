{
	"name": "notificadoresapp",
	"private": true,
	"homepage": "https://mellyzoo.github.io/notificadoresApp/",
	"version": "0.0.0",
	"type": "module",
	"packageManager": "pnpm@9.6.0",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"preview": "vite preview",
		"lint": "biome lint src",
