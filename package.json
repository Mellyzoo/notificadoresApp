	},
	"dependencies": {
		"leaflet": "^1.9.4",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-leaflet": "^4.2.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@types/leaflet": "^1.9.12",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react-swc": "^3.7.0",
		"typescript": "^5.5.4",
		"vite": "^5.4.1"
	}
}
